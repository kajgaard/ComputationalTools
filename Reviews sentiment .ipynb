   "source": [
    "import pandas as pd\n",
    "df_reviews = pd.read_pickle(r'reviewsFromImdb/networkData3.pkl') #Read in the dataframe with reviews constructed from the \"RetrievingMoviesAndReviews\" notebook\n",
    "df_reviews"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "f59f83ac",
   "metadata": {},
   "outputs": [],
   "source": [
    "from nltk.tokenize import RegexpTokenizer\n",
    "tokenizer = RegexpTokenizer(r'\\w+') #Create tokenizer with nltk library which only identifies words"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "51bac1ad",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Reviews are split up into tokens which are words in order to be able to match tokens to the sentiment list\n",
    "\n",
    "sent_tokens = []                                       #Initialize sentiment token list which holds reviews for a movie\n",
    "for row in df_reviews[\"Reviews\"]:                      #Run trough all rows in the review dataframe\n",
    "    tokens = []                                        #Initialize token list which will hold each word of a review\n",
    "    for sub in row:                                    #For each review in row\n",
    "        tokens.append(tokenizer.tokenize(sub.lower())) #add tokens to list\n",
    "    sent_tokens.append(tokens)                         #add list of tokens to list\n",
    "df_reviews['Review_tokens'] = sent_tokens              #add list of lists to dataframe column\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "2322c3b2",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_sentimentValues = pd.read_csv('Sentiment_values.txt', sep='\\t',engine='python') #Read the sentiment dataframe\n",
    "df_word_and_scores = df_sentimentValues[['word', 'happiness_average']].copy() #Extract interesting columns\n",
    "sentiment_dict = dict(zip(df_word_and_scores.word, df_word_and_scores.happiness_average)) #Make dictionary - Keys = words, values = sentiment score"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "a2ed6dc2",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Get the amount of sentiment eligible words for each review and store them in a list which will all be stored in 'ext_list' \n",
    "\n",
    "ext_list = []\n",
    "for row in df_reviews['Review_tokens']:\n",
    "    review_in_list =[]\n",
    "    for reviews in row:\n",
    "        tokens_in_list = []\n",
    "        for token in reviews:\n",
    "            if token in sentiment_dict.keys():\n",
    "                tokens_in_list.append(token)\n",
    "        review_in_list.append(len(tokens_in_list))\n",
    "    ext_list.append(review_in_list)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "715b5d30",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Similairly extract the sentiment score for each review and store tem in a list which will all be store in the dataframe column 'Sentiment'\n",
    "\n",
    "reviews_for_movie =[]\n",
    "for row in df_reviews['Review_tokens']:\n",
    "    sentiments_for_review = []\n",
    "    for review in row:\n",
    "        sentiments_for_token = []\n",
    "        for token in review:\n",
    "            if token in sentiment_dict.keys():\n",
    "                sentiments_for_token.append(sentiment_dict[token])\n",
    "        sentiments_for_review.append(int(sum(sentiments_for_token)))\n",
    "    reviews_for_movie.append(sentiments_for_review)\n",
    "df_reviews['Sentiment'] = reviews_for_movie\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "53ce5456",
   "metadata": {},
   "outputs": [],
   "source": [
    "del ext_list[1485] #Remove troublesome index\n",
    "df_reviews.drop(1485,inplace = True) #Remove troublesome row \n",
    "df_reviews = df_reviews.reset_index(drop=True) #Reset index of dataframe"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "d1e326e7",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Get average sentiment score of a movie by summing the sentiment score of a movie's reviews and dividing by the amount of sentiment eligible words in those reviews\n",
    "import numpy as np\n",
    "results = []\n",
    "for x in range(len(df_reviews['Sentiment'])):\n",
    "    results.append((sum(df_reviews['Sentiment'][x])/ len(ext_list[x])))\n",
    "df_reviews['Avg sentiment movie'] = results #Average sentiment for movie is added as column"
   ]
  },
